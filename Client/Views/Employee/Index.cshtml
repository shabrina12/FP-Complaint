
<h2>Table Employee</h2>

<table class="table table-striped table-bordered table-hover" id="tableEmployee">
    <thead class="table-head">
        <tr>
            <th scope="col" class="col-1">No</th>
            <th scope="col" class="col-1">Profile ID</th>
            <th scope="col" class="col-1">Full Name</th>
            <th scope="col" class="col-1">Gender</th>
            <th scope="col" class="col-2">Hiring Date</th>
        </tr>
    </thead>
    <tbody id="tbodyEmployee">
    </tbody>
</table>

@section Scripts{
    <script src="~/js/employee.js"></script>
    <script>
        $(document).ready(function () {
            // get token from server
            const token = '@Context.Session.GetString("JWToken")';
            sessionStorage.setItem("JWToken", token);
        });
    </script>
}